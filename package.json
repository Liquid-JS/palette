{
    "name": "@liquid-js/palette",
    "version": "0.1.3",
    "description": "Get colour palette from an image",
    "type": "module",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
        "./package.json": {
            "default": "./package.json"
        },
        ".": {
            "node": "./lib/index.js",
            "types": "./lib/index.d.ts",
            "default": "./lib/index.js"
        }
    },
    "files": [
        "lib"
    ],
    "dependencies": {
        "tinyqueue": "^3.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.3.1",
        "@commitlint/config-conventional": "^20.3.1",
        "@liquid-js/rollup-plugin-closure-compiler": "^0.28.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.3.0",
        "@stylistic/eslint-plugin": "^5.7.0",
        "clean-package": "^2.2.0",
        "commit-and-tag-version": "^12.6.1",
        "eslint": "^9.39.2",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsdoc": "^62.2.0",
        "eslint-plugin-prefer-arrow-functions": "^3.9.1",
        "eslint-plugin-unused-imports": "^4.3.0",
        "google-closure-compiler": "^20260114.0.0",
        "husky": "^9.1.7",
        "npm-check-updates": "^19.3.1",
        "rimraf": "^6.1.2",
        "rollup": "^4.55.2",
        "tslib": "^2.8.1",
        "typedoc": "^0.28.16",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.53.1",
        "vite": "^7.3.1"
    },
    "scripts": {
        "build": "rimraf lib && rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript",
        "start": "vite src",
        "lint": "eslint . --fix",
        "prepublishOnly": "npm run build",
        "prepare": "husky",
        "release": "git diff --exit-code && commit-and-tag-version && git push --follow-tags origin",
        "docs": "typedoc",
        "prepack": "clean-package",
        "postpack": "clean-package restore"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Liquid-JS/palette.git"
    },
    "keywords": [
        "color",
        "palette",
        "image"
    ],
    "author": "HitkoDev <development@hitko.si>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Liquid-JS/palette/issues"
    },
    "homepage": "https://liquid-js.github.io/palette/",
    "packageManager": "yarn@4.12.0",
    "clean-package": {
        "remove": [
            "packageManager",
            "scripts",
            "devDependencies",
            "clean-package"
        ]
    }
}
